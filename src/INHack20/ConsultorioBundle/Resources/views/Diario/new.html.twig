{% extends 'INHack20ConsultorioBundle::layout.html.twig' %}
{% block contenido %}
    <h1 class="t1 center separator-bottom">{{ 'diario.new.titulo' | trans({},'mensajes') }}</h1>
    <div class="center">
        <form action="{{ path('diario_create') }}" method="post" {{ form_enctype(form) }}>
            {{ form_widget(form) }}
            <p class="separator-top">
                <button type="submit" class="boton-v">{{ 'diario.new.submit' | trans({},'mensajes') }}</button>
            </p>
        </form>
    </div>
{% endblock %}