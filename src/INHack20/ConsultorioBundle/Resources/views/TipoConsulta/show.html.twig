{% extends 'INHack20ConsultorioBundle::layout.html.twig' %}
{% block contenido %}
    <h1>TipoConsulta</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Acronimo</th>
                <td>{{ entity.acronimo }}</td>
            </tr>
            <tr>
                <th>Significado</th>
                <td>{{ entity.significado }}</td>
            </tr>
            <tr>
                <th>Creado</th>
                <td>{{ entity.creado|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

    <ul class="record_actions">
        <li>
            <a href="{{ path('tipoconsulta') }}">
                Back to the list
            </a>
        </li>
        <li>
            <a href="{{ path('tipoconsulta_edit', { 'id': entity.id }) }}">
                Edit
            </a>
        </li>
        <li>
            <form action="{{ path('tipoconsulta_delete', { 'id': entity.id }) }}" method="post">
                {{ form_widget(delete_form) }}
                <button type="submit">Delete</button>
            </form>
        </li>
    </ul>
{% endblock %}