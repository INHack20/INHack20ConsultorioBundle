{% extends 'INHack20ConsultorioBundle::layout.html.twig' %}
{% block contenido %}
    <h1>TipoConsulta list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Acronimo</th>
                <th>Significado</th>
                <th>Creado</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('tipoconsulta_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.acronimo }}</td>
                <td>{{ entity.significado }}</td>
                <td>{% if entity.creado %}{{ entity.creado|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('tipoconsulta_show', { 'id': entity.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('tipoconsulta_edit', { 'id': entity.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipoconsulta_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
{% endblock %}