{% extends 'INHack20ConsultorioBundle::layout.html.twig' %}
{% block contenido %}
<div class="center">
  <h1 class="t1 separator-bottom">Medico</h1>
    <table class="tabla-1">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr class="odd">
            <th>Turno</th>
            <td>{{ entity.turno }}</td>
        </tr>
        <tr>
            <th>Especialidad</th>
            <td>{{ entity.especialidad }}</td>
        </tr>
        <tr class="odd">
            <th>Telefono</th>
            <td>{{ entity.telefono }}</td>
        </tr>
        <tr>
            <th>Cedula</th>
            <td>{{ entity.cedula }}</td>
        </tr>
        <tr class="odd">
            <th>Nombrecompleto</th>
            <td>{{ entity.nombreCompleto }}</td>
        </tr>
        <tr>
            <th>Edad</th>
            <td>{{ entity.edad }}</td>
        </tr>
        <tr class="odd">
            <th>Sexo</th>
            <td>{{ entity.sexo }}</td>
        </tr>
        <tr>
            <th>Creado</th>
            <td>{{ entity.fechaCreado|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr class="odd">
            <th>Modificado</th>
            <td>{{ entity.fechaModificado|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>
    <div class="separator-top">
            <a class="boton-v" href="{{ path('medico_edit', { 'id': entity.id }) }}">
                Editar
            </a>
                &nbsp;
            <a class="boton-v" href="{{ path('medico') }}">
                Regresar
            </a>
    </div>
</div>
{% endblock %}