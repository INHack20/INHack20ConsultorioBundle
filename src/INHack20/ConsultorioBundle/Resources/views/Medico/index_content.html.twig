<table class="records_list tabla-1">
    <thead>
        <tr>
            <th>Id</th>
            <th>Turno</th>
            <th>Especialidad</th>
            <th>Telefono</th>
            <th>Cedula</th>
            <th>Nombrecompleto</th>
            <th>Sexo</th>
            <th>Creado</th>
            <th>Acciones</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('medico_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.turno }}</td>
            <td>{{ entity.especialidad }}</td>
            <td>{{ entity.telefono }}</td>
            <td>{{ entity.cedula }}</td>
            <td>{{ entity.nombreCompleto }}</td>
            <td>{{ entity.sexo }}</td>
            <td>{% if entity.fechaCreado %}{{ entity.fechaCreado|date('Y-m-d') }}{% endif %}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('medico_show', { 'id': entity.id }) }}">Ver</a>
                    </li>
                    <li>
                        <a href="{{ path('medico_edit', { 'id': entity.id }) }}">Editar</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>